{
    "compact": false,
    "presets": [
        ["@babel/preset-env", {"loose": true, "targets": {"esmodules": true}}],
        ["@babel/typescript", {"isTSX": true, "allExtensions": true}]
    ],
    "plugins": [
        ["babel-plugin-inferno", {"imports": true}], 
        ["@babel/plugin-proposal-class-properties", {"loose": true}]
    ]
}